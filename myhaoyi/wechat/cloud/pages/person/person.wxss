/* pages/person/person.wxss */
Page {
  background-color: #eee;
}
.weui-cell_active {
  background-color: #dedede;
}
.feed-head {
  font-size: 32rpx;
  text-align: center;
  margin: 80rpx auto 40rpx;
}
.feed-head image {
  vertical-align: middle;
  display: inline-block;
  border-radius: 100%;
  margin-bottom: 20rpx;
  border: none;
  width: 160rpx;
  height: 160rpx;
}
.feed-name {
  color: #666;
}
.feed-icon {
  margin-right: 20rpx;
  font-size: 40rpx;
}
.feed-blue {
  color: deepskyblue;
}
.feed-green {
  color: forestgreen;
}
.feed-orange {
  color: darkorange;
}
.feed-red {
  color: tomato;
}
.feed-share {
  opacity: 0;
  width:100%;
  height: 100%;
  z-index: 9999;
  position: absolute;
}