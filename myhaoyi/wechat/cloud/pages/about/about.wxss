/* pages/about/about.wxss */
Page {
  background-color: #fff;
}
.feed-head {
  text-align: center;
  margin: 20rpx auto 20rpx;
}
.feed-head image {
  vertical-align: middle;
  display: inline-block;
  border-radius: 10rpx;
  margin-bottom: 10rpx;
  border: none;
  width: 120rpx;
  height: 120rpx;
}
.feed-name {
  /*color: #333;*/
  font-size: 32rpx;
  font-weight: 600;
}
.feed-text {
  color: #555;
  font-size: 26rpx;
  text-indent: 50rpx;
  margin: 10rpx 20rpx 0;
}
.feed-link {
  margin-top: -4rpx;
}
.feed-contact {
  color: #888;
  font-size: 28rpx;
  text-align: center;
}
.feed-hover {
  color: dodgerblue;
  /*background-color: #dedede;*/
}